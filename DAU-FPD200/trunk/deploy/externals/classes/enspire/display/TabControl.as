import enspire.display.controls.BaseControl;import enspire.display.TabSet;class enspire.display.TabControl extends BaseControl{	private var mcText:MovieClip;	public var mcPanel:MovieClip;	private var ts:TabSet;	function TabControl() {		super();	}	public function init() {		this.mcSelected._visible = false;		this.onRollOver = function() { 			this.gotoAndPlay("over");		}		this.onReleaseOutside = function() {			this.gotoAndPlay("out");		}		this.onRollOut = function () {			this.gotoAndPlay("out");		}		this.onRelease = function(){			this.gotoAndStop("overNoOut");			this.ts.setSelected(this.sName);		}		this.onPress = function() {			this.gotoAndStop("overNoOut");		}	}	public function setTabSet(ts:TabSet) {		this.ts = ts;	}	public function setPanel(mcPanel) {		this.mcPanel = mcPanel;		this.mcPanel._visible = false;	}	public function getPanel() {		return this.mcPanel;	}	public function select() {		this.enabled = false;		this.tabEnabled = false;		this.mcSelected._visible = true;		this.mcPanel._visible = true;	}	public function deselect() {		this.enabled =  true ;		this.tabEnabled = true;		this.gotoAndStop("up");		this.mcSelected._visible = false;		this.mcPanel._visible = false;	}	public function setEnabled(b) {		if(b) {			this.gotoAndStop("disabled");		}else{			this.gotoAndStop("up");		}		this.enabled = b;		this.tabEnabled = b;	}	function toString() {		return "enspire.display.TabControl";	}}